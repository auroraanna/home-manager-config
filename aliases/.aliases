alias speed='librespeed-cli'
alias neo='neofetch'
alias fetch='freshfetch'
alias buytime='dd if=/dev/urandom of=homework.pdf bs=1M count=4'
alias poweroff='systemctl poweroff'
alias suspend='systemctl suspend'
alias restart='systemctl restart'
alias yt='ytfzf'
alias bonsai='cbonsai -l -t 0.01 -i -w 0.5 -m BonsaiDisIs -M 6 -L 48 -v'
alias rainbowpuke='cat /dev/random | base64 -w 0 | lolcat'
alias uwu='uwufetch'
alias meip='curl ifconfig.me'
alias reload-keys='sudo systemd-hwdb update && sudo udevadm trigger --verbose --sysname-match="event*"'
alias keycodes='nvim /usr/include/linux/input-event-codes.h'
alias edit-keycodes='sudo $EDITOR /etc/udev/hwdb.d/90-extra-keyboard.hwdb'
alias rtop='radeontop'
alias update-grub='grub-mkconfig -o /boot/grub/grub.cfg'
alias stream='scrcpy & sndcpy'
alias hp-setup='nix run nixpkgs.hplipWithPlugin -c sudo hp-setup'
